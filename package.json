    "@types/leaflet.markercluster": "^1.4.2",
    "@zxing/ngx-scanner": "3.0.0",
    "angular2-counto": "^1.2.5",
    "blob": "0.1.0",
    "chart.js": "^2.9.3",
    "classlist.js": "^1.1.20150312",
    "color": "^3.1.2",
