{
  "name": "Humansis",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node --max-old-space-size=8192 ./node_modules/.bin/ng serve --poll 2000",
    "build": "node --max-old-space-size=8192 ./node_modules/.bin/ng build",
    "test": "ng test",
    "test:dev": "ng test --karma-config src/karma-dev.conf.js",
    "extract-translations": "node scripts/extract-translations.js",
    "serve": "ng serve",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "precommit": "lint-staged",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && pretty-quick --staged"
    }
  },
  "private": true,
  "dependencies": {
